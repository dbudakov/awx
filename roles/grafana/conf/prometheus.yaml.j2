scrape_configs:
  - job_name: 'tower'
    tls_config:
        insecure_skip_verify: True
    metrics_path: '/api/v2/metrics/'
    scrape_interval: 1s
    scheme: https
    basic_auth:
     username: {{ TOWER_USER }}
     password: {{ TOWER_PASSWORD }}
    static_configs:
        - targets:
            - 192.168.0.111